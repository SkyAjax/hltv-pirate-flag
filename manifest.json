{
  "manifest_version": 3,
  "name": "HLTV Pirate Flag Swap",
  "version": "1.2.0",
  "description": "Replaces Russian flags on hltv.org with a pirate flag",
  "content_scripts": [
    {
      "matches": ["*://*.hltv.org/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["pirate.png"],
      "matches": ["*://*.hltv.org/*"]
    }
  ]
}
